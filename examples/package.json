{
  "author": "Antonio De Luca <contact@antoniodeluca.info>",
  "description": "DN2A - Dynamic Neural Networks Architect - Examples",
  "devDependencies": {
    "@parcel/plugin": "^2.8.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "npm-run-all2": "^6.0.4",
    "parcel": "^2.8.2",
    "parcel-resolver-ts-base-url": "^1.3.1",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.3.4"
  },
  "keywords": [
    "neural networks",
    "machine learning",
    "artificial intelligence"
  ],
  "license": "MIT",
  "name": "dn2a-examples",
  "peerDependencies": {
    "dn2a": "0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antoniodeluca/dn2a-examples.git"
  },
  "scripts": {
    "format": "prettier ./assets/**/*.{ts,tsx,html,css} --write",
    "lint": "eslint ./assets/**/*.{ts,tsx}",
    "link": "ls -s ../library ./node_modules/dn2a",
    "start": "npx npm-run-all start:*",
    "start:browser": "npx npm-run-all start:browser:*",
    "start:cli": "npx npm-run-all start:cli:*",
    "start:browser:alpha-continuous:parcel": "npx parcel --no-cache assets/browser-test-network-alpha-continuous/index.html",
    "start:browser:alpha-continuous:vite": "npx vite assets/browser-test-network-alpha-continuous/",
    "start:browser:alpha-stepbystep:parcel": "npx parcel --no-cache assets/browser-test-network-alpha-stepbystep/index.html",
    "start:browser:alpha-stepbystep:vite": "npx vite assets/browser-test-network-alpha-stepbystep/",
    "start:cli:alpha-continuous-0": "npx ts-node assets/cli-test-network-alpha-continuous-0.ts",
    "start:cli:alpha-continuous-1": "npx ts-node assets/cli-test-network-alpha-continuous-1.ts",
    "start:cli:alpha-continuous-2": "npx ts-node assets/cli-test-network-alpha-continuous-2.ts",
    "start:cli:alpha-continuous-3": "npx ts-node assets/cli-test-network-alpha-continuous-3.ts",
    "start:cli:alpha-continuous-4": "npx ts-node assets/cli-test-network-alpha-continuous-4.ts",
    "start:cli:alpha-stepbystep-0": "npx ts-node assets/cli-test-network-alpha-stepbystep-0.ts"
  },
  "version": "0.1.2"
}
